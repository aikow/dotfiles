{
  "editor.fontFamily": "FiraCode Nerd Font",
  "editor.fontLigatures": true,
  "terminal.integrated.fontFamily": "Hack Nerd Font",
  "editor.minimap.enabled": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Nord",
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.defaultDateFormat": "Do MMMM YYYY, HH:MM",
  "gitlens.defaultDateShortFormat": "D MMM YYYY",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.activityBar.visible": true,
  "projectManager.git.baseFolders": [
    "$HOME/Documents",
    "$HOME/Developer"
  ],
  "python.languageServer": "Pylance",
  "editor.multiCursorModifier": "alt",
  "git.ignoreMissingGitWarning": true,
  "terminal.integrated.tabs.enabled": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "settingsSync.ignoredSettings": [
    "editor.fontFamily",
    "terminal.integrated.fontFamily"
  ],
  "security.workspace.trust.untrustedFiles": "newWindow",
  "editor.wordWrap": "on",
  "cSpell.userWords": [
    "Dotfiles",
    "segmentor"
  ],
  "editor.fontSize": 13,
  "terminal.integrated.fontSize": 13,
  "keyboard.dispatch": "keyCode",
  "vim.enableNeovim": true,
  "vim.foldfix": true,
  "vim.leader": "<space>",
  "vim.neovimUseConfigFile": true,
  "vim.neovimConfigPath": "$HOME/.config/nvim/init.vim",
  "vim.neovimPath": "/usr/bin/nvim",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["]", "e"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["[", "e"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["]", "e"],
      "commands": ["editor.action.marker.nextInFiles"]
    },
    {
      "before": ["[", "e"],
      "commands": ["editor.action.marker.prevInFiles"]
    },
    {
      "before": ["<leader>", "b"],
      "commands": ["bookmarks.list"]
    },
    {
      "before": ["<leader>", "m"],
      "commands": ["bookmarks.toggle"]
    },
    {
      "before": ["<leader>", "p"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["<leader>", "o"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["leader", "w"],
      "commands": ["workbench.action.files.save"]
    }
  ],
  "vim.smartRelativeLine": true,
  "vim.vimrc.enable": false,
  "vim.vimrc.path": "$HOME/.config/nvim/init.vim",
  "terminal.integrated.defaultProfile.linux": "/usr/bin/zsh",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.tabSize": 2,
}
