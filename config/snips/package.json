{
  "name": "aiko-vscode-snippets",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "all"
        ],
        "path": "./snippets/all.json"
      },
      {
        "language": [
          "sh",
          "bash"
        ],
        "path": "./snippets/bash.json"
      },
      {
        "language": [
          "fish"
        ],
        "path": "./snippets/fish.json"
      },
      {
        "language": [
          "lua"
        ],
        "path": "./snippets/lua.json"
      },
      {
        "language": [
          "norg"
        ],
        "path": "./snippets/norg.json"
      },
      {
        "language": [
          "python"
        ],
        "path": "./snippets/python.json"
      },
      {
        "language": [
          "rust"
        ],
        "path": "./snippets/rust.json"
      }
    ]
  }
}
