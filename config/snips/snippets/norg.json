{
  "code": {
    "description": "code_block",
    "prefix": "code",
    "body": ["@code ${1:lang}", "$0", "@end"]
  },
  "math": {
    "description": "math block",
    "prefix": "math",
    "body": ["@math", "$0", "@end"]
  },
  "table": {
    "description": "table",
    "prefix": "table",
    "body": ["@table", "$0", "@end"]
  },
  "task": {
    "description": "task",
    "prefix": "task",
    "body": ["- ( ) ${0:task}"]
  },
  "link": {
    "description": "links",
    "prefix": "link",
    "body": ["{${1:description}}[${2:object}]"]
  },
  "data": {
    "description": "data tags",
    "prefix": "data",
    "body": ["@data", "$0", "@end"]
  },
  "bold": {
    "description": "bold words",
    "prefix": "bold",
    "body": ["*$1*"]
  },
  "italic": {
    "description": "italic words",
    "prefix": "italic",
    "body": ["/$1/"]
  },
  "underline": {
    "description": "underline words",
    "prefix": "underline",
    "body": ["_$1_"]
  },
  "subscript": {
    "description": "subscript",
    "prefix": "subscript",
    "body": [",$1,"]
  },
  "superscript": {
    "description": "superscript",
    "prefix": "superscript",
    "body": ["^$1^"]
  },
  "var": {
    "description": "variable",
    "prefix": "var",
    "body": ["=${1:variable}="]
  }
}
