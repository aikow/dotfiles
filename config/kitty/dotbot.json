[
  {
    "defaults": {
      "link": {
        "if": "command -v kitty &>/dev/null",
        "relink": true,
        "force": true
      }
    }
  },
  {
    "link": {
      "~/.config/kitty/kitty.conf": {
        "path": "kitty.conf"
      }
    }
  },
  {
    "link": {
      "~/.config/kitty/open-actions.conf": {
        "path": "open-actions.conf"
      }
    }
  }
]
